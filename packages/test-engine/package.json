{
  "name": "@agelum/test-engine",
  "version": "1.0.0",
  "private": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch"
  },
  "dependencies": {
    "@agelum/llm-provider": "workspace:*",
    "agent-browser": "latest",
    "zod": "latest"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@types/node": "^20.0.0",
    "playwright": "^1.58.2",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "description": "Browser testing engine for Agelum",
  "license": "MIT"
}
