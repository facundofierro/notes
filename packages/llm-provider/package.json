{
  "name": "@agelum/llm-provider",
  "version": "1.0.0",
  "private": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.41",
    "@ai-sdk/google": "^3.0.23",
    "@ai-sdk/openai": "^3.0.26",
    "@ai-sdk/xai": "^3.0.50",
    "ai": "^3.4.0",
    "openai": "^4.20.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "description": "LLM Provider abstraction for Agelum",
  "license": "MIT"
}
